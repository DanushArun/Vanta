# Vanta Environment Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Use a proxy server for API key security (recommended for production)
# GEMINI_PROXY_URL=https://your-proxy.example.com/gemini

# Feature Flags
ENABLE_DEBUG_LOGGING=true
ENABLE_ML_SAFETY=false

# Audio Configuration (override defaults if needed)
# AUDIO_INPUT_SAMPLE_RATE=16000
# AUDIO_OUTPUT_SAMPLE_RATE=24000

# Camera Configuration (override defaults if needed)
# CAMERA_FRAME_RATE=2
# CAMERA_JPEG_QUALITY=50
